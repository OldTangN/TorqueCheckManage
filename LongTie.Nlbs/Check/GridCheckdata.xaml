<Grid  x:Class="LongTie.Nlbs.Check.GridCheckdata"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
       Height="128" Width="235">
    <Grid HorizontalAlignment="Left" >
        <DataGrid Name="chekshow" AutoGenerateColumns="False" IsEnabled="False"  CanUserAddRows="False" CanUserSortColumns="False"  HeadersVisibility="None"  >
            <DataGrid.Columns >
                <!--<DataGridTextColumn IsReadOnly="True"  Header="序号" Width=" 52" Binding="{Binding Path=id}"></DataGridTextColumn>-->
                <DataGridTextColumn IsReadOnly="True" Header="目标" Visibility="Hidden"  Width=" 60"  Binding="{Binding Path=targetvalue}"></DataGridTextColumn>
                <DataGridTextColumn IsReadOnly="True" Header="校验值"  Width="60" Binding="{Binding Path=checkvalue}"></DataGridTextColumn>
                <DataGridTextColumn IsReadOnly="True" Header="误差" Width="60" Binding="{Binding Path=resultrate}"></DataGridTextColumn>
                <DataGridTextColumn Header="结果" Width="60" Binding="{Binding Path=isgood}"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Grid>
